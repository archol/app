{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-typescript",
      {
        "allExtensions": true,
        "isTSX": true
      }
    ],
    [
      "babel-plugin-debug-tools",
      {
        "mode": "DEV"
      }
    ],
    "babel-plugin-minify-flip-comparisons"
  ]
}